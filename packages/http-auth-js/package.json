{
  "name": "@icp-sdk/http",
  "version": "0.0.0",
  "description": "Client side support for ICP HTTP authentication",
  "author": "DFINITY Stiftung",
  "license": "Apache-2.0",
  "homepage": "https://github.com/dfinity/http-auths#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dfinity/http-auth.git",
    "directory": "packages/http-auth-js"
  },
  "bugs": {
    "url": "https://github.com/dfinity/http-auth/issues"
  },
  "keywords": [
    "internet",
    "computer",
    "icp",
    "dfinity",
    "http",
    "auth",
    "authentication"
  ],
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.cts"
      },
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./auth": {
      "types": {
        "import": "./dist/esm/auth/index.d.ts",
        "require": "./dist/cjs/auth/index.d.cts"
      },
      "import": "./dist/esm/auth/index.js",
      "require": "./dist/cjs/auth/index.cjs",
      "default": "./dist/esm/auth/index.js"
    }
  },
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "@dajiaji/bhttp": "jsr:^0.3.6",
    "@icp-sdk/core": "^4.2.1",
    "@noble/hashes": "^1.8.0"
  }
}
